;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     stembayo.id. root.stembayo.id. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      stembayo.id.
43      IN      PTR     stembayo.id.